script/source = "extends CharacterBody2D

var max_speed = 100
var last_direction = Vector2(1, 0)

func _physics_process(delta: float) -> void:
	var direction = Input.get_vector(\"move_left\", \"move_right\", \"move_up\", \"move_down\")

	# Determine running or walking
	var is_running = Input.is_action_pressed(\"run\")
	max_speed = 140 if is_running else 100
	
	# Allow movement in only one direction
	if abs(direction.x) > 0:
		direction.y = 0
	elif abs(direction.y) > 0:
		direction.x = 0

	velocity = direction * max_speed
	move_and_slide()

	if direction.length() > 0:
		last_direction = direction
		
		if is_running:
			play_running_animation(direction)
		else:
			play_walk_animation(direction)
	else:
		$AnimatedSprite2D.stop()

func play_walk_animation(direction):
	if direction.x > 0:
		$AnimatedSprite2D.play(\"walking_right\")
	elif direction.x < 0:
		$AnimatedSprite2D.play(\"walking_left\")
	elif direction.y < 0:
		$AnimatedSprite2D.play(\"walking_up\")
	elif direction.y > 0:
		$AnimatedSprite2D.play(\"walking_down\")

func play_running_animation(direction):
	if direction.x > 0:
		$AnimatedSprite2D.play(\"running_right\")
	elif direction.x < 0:
		$AnimatedSprite2D.play(\"running_left\")
	elif direction.y < 0:
		$AnimatedSprite2D.play(\"running_up\")
	elif direction.y > 0:
		$AnimatedSprite2D.play(\"running_down\")
"

